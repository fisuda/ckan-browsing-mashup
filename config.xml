        <connection sourcename="widget/93/highcharts-graph-config" targetname="widget/94/highcharts"/>
      </visualdescription>
    </wiring>
  </structure>
  <embedded>
    <resource name="data-viewer-table" src="macs/CoNWeT_data-viewer-table_2.0.0.wgt" vendor="CoNWeT" version="2.0.0"/>
